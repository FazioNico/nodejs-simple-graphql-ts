{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "./dist",
    "lib": [
      "dom",
      "es2015",
      "es2015.iterable",
      "esnext.asynciterable"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    ".ideas",
    "node_modules",
    "dist"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
